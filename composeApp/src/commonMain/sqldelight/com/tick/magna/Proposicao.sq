CREATE TABLE Proposicao (
    id TEXT NOT NULL PRIMARY KEY,
    codTipo TEXT,
    ementa TEXT,
    dataApresentacao TEXT,
    autores TEXT,
    url TEXT
);

insertProposicao:
INSERT OR REPLACE INTO Proposicao VALUES ?;

getProposicoes:
SELECT * FROM Proposicao ORDER BY dataApresentacao DESC LIMIT 3;

getProposicoesByCodTipo:
SELECT * FROM Proposicao WHERE codTipo = :codTipo ORDER BY dataApresentacao DESC LIMIT 3;