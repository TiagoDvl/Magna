CREATE TABLE IF NOT EXISTS User (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    legislaturaid TEXT,
    last_sync TEXT,
    FOREIGN KEY(legislaturaid) REFERENCES Legislatura(id)
);

-- Create
insertUser:
INSERT OR REPLACE INTO User(id, legislaturaid, last_sync)
VALUES (?, ?, ?);

-- Read all
selectAllUsers:
SELECT * FROM User;

-- Read by ID
selectUserById:
SELECT * FROM User WHERE id = ?;

-- Update
updateUserLegislatura:
UPDATE User
SET legislaturaid = ?
WHERE id = 1;

-- Delete
deleteUserById:
DELETE FROM User WHERE id = ?;